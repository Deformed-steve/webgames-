    <script>
  $(function () {
    const $iframe = $("#ai-chat-frame");
    if (!$iframe.length) return;
    $iframe.on("load", () => {
      $(".loading-placeholder").remove();
      const observer = new IntersectionObserver((entries, obs) => {
        if (entries[0].isIntersecting) {
          $iframe[0].contentWindow.runRandomDemo();
          setTimeout(() => $iframe[0].contentWindow.runRandomDemo(), 6000);
          obs.disconnect();
        }
      }, { rootMargin: "0px", threshold: 0.5 });
      observer.observe($iframe[0]);
    });
    if ($iframe.data("src")) {
      $iframe.attr("src", $iframe.data("src"));
    }
  });
  </script>
  
